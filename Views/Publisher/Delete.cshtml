@model BookStore1.Models.Publisher

<h1>Delete Publisher</h1>

<h3>Are you sure you want to delete this publisher?</h3>
<div>
    <h4>ID: @Model.Id</h4>
    <h4>Name: @Model.Name</h4>
    <h4>Location: @Model.Location</h4>
</div>

<form asp-action="Delete">
    <input type="hidden" asp-for="Id" />

    <button type="submit" class="btn btn-danger">Delete</button> |
    <a asp-action="Index" asp-controller="Publisher">Back to List</a>
</form>

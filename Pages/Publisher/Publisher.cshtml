
@model IEnumerable<BookStore1.Models.Publisher>


<h2>Publishers</h2>
<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var publisher in Model)
        {
            <tr>
                <td>@publisher.Name</td>
                <td>
                    <a asp-action="Edit" asp-route-id="@publisher.Id">Edit</a> |
                    <a asp-action="Delete" asp-route-id="@publisher.Id">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
<a asp-action="Create">Create New Publisher</a>
@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}
